<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="approval">

    <select id="toList" resultType="com.end2end.spring.approval.dto.ApprovalDTO">
        SELECT * FROM approvals ORDER BY regDate DESC
    </select>
    <select id="write" resultType="com.end2end.spring.employee.dto.EmployeeDTO">
        select * from employee where DEPARTMENTID = ${departmentId};
    </select>
</mapper>